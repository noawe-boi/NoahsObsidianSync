		{"id":"94f45ec8c0e0d55f","type":"text","text":"Sacricices beyond the physical realm exist for both. Consider epistemic sacrifices","x":-4327,"y":-2890,"width":533,"height":165},
		{"id":"d96491887a2de89b","x":-3528,"y":-2837,"width":250,"height":60,"type":"text","text":"Epistmic"},
